
{
  "let-in": {
    "prefix": "let",
    "body": [
      "let",
      "    $0",
      "in"
    ]
  },
  "noop": {
    "prefix": "noop",
    "body": [
      "( ${1:model}",
      ", ${2:Effects.none}",
      ")"
    ]
  },
  "model": {
    "prefix": "m",
    "body": [
      "{ ${1:model} | ${2:field} = ${0:value} }"
    ]
  }
}
