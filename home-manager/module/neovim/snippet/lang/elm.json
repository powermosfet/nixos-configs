
{
  "log": {
    "prefix": "log",
    "body": [
      "(Debug.log \"${1:$TM_SELECTED_TEXT}\" ($1))"
    ]
  },
  "documentation": {
    "prefix": "doc",
    "body": [
      "{-| $0",
      "-}"
    ]
  },
  "let-in": {
    "prefix": "letin",
    "body": [
      "let",
      "    $0",
      "in"
    ]
  },
  "noop": {
    "prefix": "noop",
    "body": [
      "( ${1:model}",
      ", ${2:Effects.none}",
      ")"
    ]
  },
  "model": {
    "prefix": "m",
    "body": [
      "{ ${1:model} | ${2:field} = ${3:${2}} }"
    ]
  }
}
